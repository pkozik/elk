{
  "template": "eas_application_reports-*",
  "mappings": {
    "log": {
      "properties": {
        "@timestamp": {
          "type":"date",
          "format": "strict_date_optional_time||epoch_millis"
        },
        "application": {
          "type": "keyword",
          "index": "true"
        },
        "type": {
          "type": "keyword",
          "index": "true"
        },
        "program": {
          "type": "keyword"
        },
        "host": {
          "type": "keyword",
          "index": "true"
        },
        "log4j_priority": {
          "type": "keyword"
        },
        "priority": {
          "type": "keyword"
        },
        "thread": {
          "type": "keyword",
          "index": "false"
        },
        "user": {
          "type": "keyword",
          "index": "true"
        },
        "userid": {
          "type": "keyword",
          "index": "true"
        },
        "chatthreadid": {
          "type": "keyword",
          "index": "true"
        },
        "conferenceId": {
          "type": "keyword",
          "index": "true"
        },
        "EAS_ALARM": {
          "type": "keyword",
          "index": "true"
        },
        "EAS_EVENT": {
          "type": "keyword",
          "index": "true"
        },
        "EAS_INFO": {
          "type": "keyword",
          "index": "true"
        }
      }
    }
  }
}